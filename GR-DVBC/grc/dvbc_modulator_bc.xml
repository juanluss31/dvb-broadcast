<?xml version="1.0"?>
<block>
  <name>DVB-C Modulator</name>
  <key>dvbc_modulator_bc</key>
  <category>[Core]/Digital Television/DVB-C</category>
  <import>import dvbc</import>
  <make>dvbc.modulator_bc($constellation.val)</make>
  <param>
    <name>Constellation</name>
    <key>constellation</key>
    <type>enum</type>
    <option>
      <name>16QAM</name>
      <key>MOD_16QAM</key>
      <opt>val:dvbc.MOD_16QAM</opt>
    </option>
    <option>
      <name>32QAM</name>
      <key>MOD_12QAM</key>
      <opt>val:dvbc.MOD_32QAM</opt>
    </option>
    <option>
      <name>64QAM</name>
      <key>MOD_64QAM</key>
      <opt>val:dvbc.MOD_64QAM</opt>
    </option>
    <option>
      <name>128QAM</name>
      <key>MOD_128QAM</key>
      <opt>val:dvbc.MOD_128QAM</opt>
    </option>
    <option>
      <name>256QAM</name>
      <key>MOD_256QAM</key>
      <opt>val:dvbc.MOD_256QAM</opt>
    </option>
  </param>
  <sink>
    <name>in</name>
    <type>byte</type>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
